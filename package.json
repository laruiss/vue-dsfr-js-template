{
  "name": "vue-project-js",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p build-only",
    "build-only": "vite build",
    "dev": "vite",
    "format": "run-s format:prettier format:eslint",
    "format:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format:prettier": "prettier --write src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "pretest:e2e": "npm run build",
    "pretest:e2e:ci": "npm run build",
    "preview": "vite preview --host --port 5050",
    "test": "run-s test:unit test:ct:ci test:e2e:ci",
    "test:ct": "cypress open --component",
    "test:ct:ci": "cypress run --component",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.10.0-rc.1",
    "@gouvminint/vue-dsfr": "^4.0.2",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@cypress/vue": "^5.0.5",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^7.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.3.2",
    "cypress": "^12.14.0",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "unplugin-auto-import": "^0.16.4",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.4",
    "vitest": "^0.32.0",
    "workbox-window": "^7.0.0"
  }
}
